<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape>
            <stroke
                android:width="1dp"
                android:color="#333969EF" />
            <corners android:radius="@dimen/smallShapeCornerRadius" />
        </shape>
    </item>
    <item  android:right="1dp" android:left="1dp" android:top="1dp" android:bottom="1dp">
        <shape>
            <stroke
                android:width="1dp"
                android:color="#3969EF" />
            <corners android:radius="@dimen/smallShapeCornerRadius" />
        </shape>
    </item>
    <item>
        <shape>
            <solid android:color="?attr/daxColorContainer" />
            <corners android:radius="@dimen/smallShapeCornerRadius" />
        </shape>
    </item>
</layer-list>

